            <field name="act_from" ref="a_0F17_24E7"/>
            <field name="act_to" ref="a_0F17_1F36"/>
            <field name="signal">sgn_to_draft</field>
            <field name="condition">True</field>
        </record>
        <record id="t_0F17_0F38" model="workflow.transition">
