<openerp>
    <data>

        <!-- SEARCH VIEW -->
        <record id="view_sondaggio_estrato_filter" model="ir.ui.view">
            <field name="name">sondaggio.estrato.select</field>
            <field name="model">sondaggio.estrato</field>
            <field name="arch" type="xml">
                <search string="estrato" version="7.0">
                    <group string="By Attribute">
                    <field name="name" string="name"
                            />
                    <field name="sector" string="sector"
                            />
                    <field name="dimension" string="Dim"
                            />
                    <field name="region" string="Reg"
                            />
                    <field name="var_es_4" string="var_es_4"
                            />
                    <field name="var_es_5" string="var_es_5"
                            />
                    <field name="tam_muestra" string="tam_muestra"
                            />
                    <field name="cant_muestra_reemplazo" string="cant_muestra_reemplazo"
                            />
                    </group>
                    <group string="By object">
                    </group>
                </search>
            </field>
        </record>

        <!-- FORMVIEW -->
        <record id="view_sondaggio_estrato_form" model="ir.ui.view">
            <field name="name">sondaggio.estrato.form</field>
            <field name="model">sondaggio.estrato</field>
            <field name="arch" type="xml">
                <form string="estrato" version="7.0">
                    <header>
                    </header>
                    <sheet string="estrato">
                     <group>
                        <field name="name"
                            />
                        <field name="sector"
                            />
                        <field name="dimension"
                            />
                        <field name="region"
                            />
                        <field name="var_es_4"
                            />
                        <field name="var_es_5"
                            />
                        <field name="tam_muestra"
                            />
                        <field name="cant_muestra_reemplazo"
                            />
                    <field name="questionnaire_ids"
                            />
                    <field name="survey_id"
                            />
                     </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->

        <record id="view_sondaggio_estrato_tree" model="ir.ui.view">
            <field name="name">sondaggio.estrato.tree</field>
            <field name="model">sondaggio.estrato</field>
            <field name="arch" type="xml">
                <tree string="estrato"
                    >
                    <field name="name"
                        />
                    <field name="sector"
                        />
                    <field name="dimension"
                        />
                    <field name="region"
                        />
                    <field name="var_es_4"
                        />
                    <field name="var_es_5"
                        />
                    <field name="tam_muestra"
                        />
                    <field name="cant_muestra_reemplazo"
                        />
                    <field name="survey_id"
                        />
                </tree>
            </field>
        </record>

        <record id="view_sondaggio_estrato_tree_edit" model="ir.ui.view">
            <field name="name">sondaggio.estrato.tree_edit</field>
            <field name="model">sondaggio.estrato</field>
            <field name="arch" type="xml">
                <tree string="estrato"
                    editable="bottom"
                    >
                    <field name="name"
                        />
                    <field name="sector"
                        />
                    <field name="dimension"
                        />
                    <field name="region"
                        />
                    <field name="var_es_4"
                        />
                    <field name="var_es_5"
                        />
                    <field name="tam_muestra"
                        />
                    <field name="cant_muestra_reemplazo"
                        />
                    <field name="survey_id"
                        />
                </tree>
            </field>
        </record>

    </data>
</openerp>
